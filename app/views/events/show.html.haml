- page_title @event.name

- if can?(:manage, @event)
  .row
    .col-md-6
      = render 'event_card', event: @event
      = render partial: 'temporary_products'
    .col-md-6
      = render partial: 'bartenders'
- else
  .row
    .col-md-offset-3.col-md-6
      = render 'event_card', event: @event
